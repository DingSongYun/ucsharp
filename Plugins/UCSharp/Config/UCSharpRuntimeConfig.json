{
	"runtimeconfig": {
		"tfm": "net6.0",
    "framework": {
      "name": "Microsoft.NETCore.App",
      "version": "8.0.0"
    },
    "configProperties": {
      "System.GC.Server": true,
      "System.GC.Concurrent": true,
      "System.Runtime.Serialization.EnableUnsafeBinaryFormatterSerialization": false
    }
  },
  "ucsharp": {
    "assemblySearchPaths": [
      "Managed",
      "Managed/Core",
      "Managed/Examples"
    ],
    "entryAssembly": "UCSharp.Managed.dll",
		"entryType": "UCSharp.Program",
		"entryMethod": "InitializeUnmanaged",
    "enableLogging": true,
    "logLevel": "Information",
    "enableDebugger": false,
    "gcSettings": {
      "serverGC": true,
      "concurrentGC": true,
      "retainVM": true
    },
    "interop": {
      "enableNativeCallbacks": true,
      "maxManagedCallStack": 1024,
      "enableExceptionHandling": true
    }
  }
}
